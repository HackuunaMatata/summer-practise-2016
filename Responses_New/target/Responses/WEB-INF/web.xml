<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ServletFormsClient</servlet-name>
    <servlet-class>Responses.servlets.ServletFormsClient</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletAdmin</servlet-name>
    <servlet-class>Responses.servlets.ServletAdmin</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletFormsClient</servlet-name>
    <url-pattern>/ServletFormsClient</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServletAdmin</servlet-name>
    <url-pattern>/ServletAdmin</url-pattern>
  </servlet-mapping>
</web-app>
