<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.factory.ContactHobbyDetailEntity" table="contact_hobby_detail" schema="javastudy">
        <composite-id mapped="true" class="hibernate.factory.ContactHobbyDetailEntityPK">
            <key-property name="contactId" column="contact_id"/>
            <key-property name="hobbyId" column="hobby_id"/>
        </composite-id>
    </class>
</hibernate-mapping>